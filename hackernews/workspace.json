{
  "version": 2,
  "cli": { "defaultCollection": "@nrwl/next" },
  "defaultProject": "hackernews-web",
  "generators": {
    "@nrwl/react": { "application": { "babel": true } },
    "@nrwl/next": { "application": { "style": "scss", "linter": "eslint" } }
  },
  "projects": {
    "hackernews-web": {
      "root": "apps/hackernews-web",
      "sourceRoot": "apps/hackernews-web",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/hackernews-web",
            "outputPath": "dist/apps/hackernews-web"
          },
          "configurations": { "production": {} }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": { "buildTarget": "hackernews-web:build", "dev": true },
          "configurations": {
            "production": {
              "buildTarget": "hackernews-web:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": { "buildTarget": "hackernews-web:build:production" }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/hackernews-web"],
          "options": {
            "jestConfig": "apps/hackernews-web/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/hackernews-web/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "hackernews-web-e2e": {
      "root": "apps/hackernews-web-e2e",
      "sourceRoot": "apps/hackernews-web-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/hackernews-web-e2e/cypress.json",
            "tsConfig": "apps/hackernews-web-e2e/tsconfig.e2e.json",
            "devServerTarget": "hackernews-web:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "hackernews-web:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/hackernews-web-e2e/**/*.js"] }
        }
      }
    },
    "hackernews-ui": {
      "root": "libs/hackernews-ui",
      "sourceRoot": "libs/hackernews-ui/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/hackernews-ui/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/hackernews-ui"],
          "options": {
            "jestConfig": "libs/hackernews-ui/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": { "configFolder": "libs/hackernews-ui/.storybook" }
          },
          "configurations": { "ci": { "quiet": true } }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/hackernews-ui",
            "config": { "configFolder": "libs/hackernews-ui/.storybook" }
          },
          "configurations": { "ci": { "quiet": true } }
        }
      }
    },
    "hackernews-ui-e2e": {
      "root": "apps/hackernews-ui-e2e",
      "sourceRoot": "apps/hackernews-ui-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/hackernews-ui-e2e/cypress.json",
            "tsConfig": "apps/hackernews-ui-e2e/tsconfig.e2e.json",
            "devServerTarget": "hackernews-ui:storybook"
          },
          "configurations": {
            "ci": { "devServerTarget": "hackernews-ui:storybook:ci" }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/hackernews-ui-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  }
}
